          "_anchor": "buttons1-a",
          "buttons": "<a data-app-btn=\"true\" class=\"mbr-buttons__btn btn btn-lg btn-danger\" href=\"yourcaninescuisine.html#pricing-table1-8\" data-toolbar=\"-mbrBtnMove\"><span class=\"mbri-credit-card mbr-iconfont mbr-iconfont-btn\" style=\"color: rgb(0, 0, 0);\"></span>BEST DOG FOOD BRANDS</a> <a data-app-btn=\"true\" class=\"mbr-buttons__btn btn btn-lg btn-primary\" href=\"http://bit.do/wellnessdogfood\" data-toolbar=\"-mbrBtnMove\" target=\"_blank\"><span class=\"mbri-cash mbr-iconfont mbr-iconfont-btn\"></span>SHOP @ AMAZON.COM</a>"
        },
        {
          "_tags": [
